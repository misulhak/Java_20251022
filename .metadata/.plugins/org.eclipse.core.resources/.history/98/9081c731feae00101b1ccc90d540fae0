package Test;

public class Example01 {

	public static void main(String[] args) {
		
		/*
		 * ----------출력 예시----------
		 * 이름 : 본인이름         (name) String name = "박진희"
		 * 나이 : 25             (age)
		 * 주소 : 서울시 천호동    (address)
		 * 몸무게 : 77.5kg      (weight)
		 */
		
		System.out.println("----------출력 예시----------");
		
		String name = "박진희";
		System.out.println("이름 : ", + name);
		
		String age = "25";
		System.out.println("나이 : ", + age);
		
		

	}

}
